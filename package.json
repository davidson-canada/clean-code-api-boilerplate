{
  "name": "clean-code-api-boilerplate",
  "version": "1.0.0",
  "description": "A (relatively) clean code base to start a Node API",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidson-canada/clean-code-api-boilerplate.git"
  },
  "author": "Davidson Canada Inc. - Alexandre Theve",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -d",
    "dev": "nodemon",
    "dev:debug": "nodemon --inspect",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "test": "jest",
    "watch": "tsc -d --watch",
    "start": "npm-run-all clean build --parallel watch dev --print-label",
    "start:docker": "docker-compose up --build",
    "gcp-build": "gcloud builds submit --tag gcr.io/dav-talk-gcr/clean-code-api-boilerplate",
    "gcp-deploy-dev": "gcloud run deploy clean-code-api-development --update-env-vars NODE_ENV=development --image gcr.io/dav-talk-gcr/clean-code-api-boilerplate --region northamerica-northeast1 --memory 512Mi --platform managed --quiet --allow-unauthenticated",
    "gcp-deploy-staging": "gcloud run deploy clean-code-api-staging --update-env-vars NODE_ENV=staging --image gcr.io/dav-talk-gcr/clean-code-api-boilerplate --region northamerica-northeast1 --memory 512Mi --platform managed --quiet --allow-unauthenticated",
    "gcp-deploy-prod": "gcloud run deploy clean-code-api-production --update-env-vars NODE_ENV=production --image gcr.io/dav-talk-gcr/clean-code-api-boilerplate --region northamerica-northeast1 --memory 512Mi --platform managed --quiet --allow-unauthenticated",
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^1.4.1",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "fast-xml-parser": "^3.16.0",
    "g": "^2.0.1",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.10.12",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.2",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "winston": "^3.2.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/mongoose": "^5.7.12",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.24",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^12.12.21",
    "@types/passport": "^1.0.3",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "lerna": "^3.20.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "ts-jest": "^24.2.0",
    "tslint": "^6.1.1",
    "typescript": "^3.7.3",
    "npm-run-all": "^4.1.5",
    "yarn": "^1.22.10",
    "rimraf": "^3.0.2"
  }
}
