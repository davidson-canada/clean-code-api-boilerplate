{
  "name": "clean-code-api-boilerplate",
  "version": "1.0.0",
  "description": "A (relatively) clean code base to start a Node API",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidson-canada/clean-code-api-boilerplate.git"
  },
  "author": "Davidson Canada Inc. - Alexandre Theve",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start": "npm-run-all clean --parallel start:dev --print-label",
    "start:docker": "docker-compose up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gcp-build": "gcloud builds submit --tag gcr.io/dav-talk-gcr/clean-code-api-boilerplate",
    "gcp-deploy-dev": "gcloud run deploy clean-code-api-development --update-env-vars NODE_ENV=development --image gcr.io/dav-talk-gcr/clean-code-api-boilerplate --region northamerica-northeast1 --memory 512Mi --platform managed --quiet --allow-unauthenticated",
    "gcp-deploy-staging": "gcloud run deploy clean-code-api-staging --update-env-vars NODE_ENV=staging --image gcr.io/dav-talk-gcr/clean-code-api-boilerplate --region northamerica-northeast1 --memory 512Mi --platform managed --quiet --allow-unauthenticated",
    "gcp-deploy-prod": "gcloud run deploy clean-code-api-production --update-env-vars NODE_ENV=production --image gcr.io/dav-talk-gcr/clean-code-api-boilerplate --region northamerica-northeast1 --memory 512Mi --platform managed --quiet --allow-unauthenticated"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.2",
    "@nestjs/common": "^7.5.2",
    "@nestjs/core": "^7.5.2",
    "@nestjs/platform-express": "^7.5.2",
    "@nestjs/testing": "^7.5.2",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^2.5.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "fast-xml-parser": "^3.17.4",
    "g": "^2.0.1",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.7",
    "@types/passport": "^1.0.4",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5",
    "npm-run-all": "^4.1.5",
    "yarn": "^1.22.10",
    "rimraf": "^3.0.2"
  }
}
